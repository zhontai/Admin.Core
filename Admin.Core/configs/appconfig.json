{
  //Api地址
  "urls": [ "http://*:8000" ],
  //跨域地址
  "corUrls": [ "http://*:9000" ],
  //Swagger文档
  "swagger": false,
  //统一认证授权服务器
  "IdentityServer": {
    //启用
    "enable": false,
    //地址，开发认证地址前往appconfig.Development.json修改
    "url": "https://login.zhontai.net"
  },
  //面向切面编程
  "aop": {
    //事物
    "transaction": true
  },
  //日志
  "log": {
    //操作日志
    "operation": true
  },
  //限流
  "rateLimit": false,
  //验证码
  "varifyCode": {
    //启用
    "enable": true,
    //字体列表
    "fonts": [ "Times New Roman", "Verdana", "Arial", "Gungsuh", "Impact" ]
  }
}
