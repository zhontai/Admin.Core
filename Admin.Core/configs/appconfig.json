{
  //Api地址
  "urls": [ "http://*:8888" ],
  //跨域地址
  "corUrls": [ "http://*:9999" ],
  //Swagger文档
  "swagger": false,
  //面向切面编程
  "aop": {
    //事物
    "transaction": true
  },
  //日志
  "log": {
    //操作日志
    "operation": true
  },
  //限流
  "rateLimit": false,
  //验证码
  "varifyCode": {
    //启用
    "enable": true,
    //字体列表
    "fonts": [ "Times New Roman", "Verdana", "Arial", "Gungsuh", "Impact" ]
  }
}
